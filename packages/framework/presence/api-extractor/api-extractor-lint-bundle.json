{
	"$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
	"extends": "<projectFolder>/../../../common/build/build-common/api-extractor-lint.json",
	// presence has no alpha/beta APIs but does have a hand-written legacy API; use that for complete bundle analysis
	"mainEntryPointFilePath": "<projectFolder>/lib/legacy.d.ts",
	"messages": {
		"extractorMessageReporting": {
			// api-extractor does not appear able to resolve self references such as
			// @fluid-experimental/presence/internal/core-interfaces without using
			// the corresponding .ts file. Suppress until core-interfaces section is
			// properly homed.
			"ae-wrong-input-file-type": {
				"logLevel": "none"
			}
		}
	}
}
